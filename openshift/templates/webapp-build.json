{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {},
  "objects": [
    {
      "kind": "BuildConfig",
      "apiVersion": "build.openshift.io/v1",
      "metadata": {
        "name": "showcase-java-web-app",
        "creationTimestamp": null,
        "labels": {},
        "annotations": {}
      },
      "spec": {
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "source": {
          "type": "Git",
          "git": {
            "uri": "https://github.com/cvarjao-o/showcase-java-web-app.git",
            "ref": "release/0.0.1"
          }
        },
        "strategy": {
          "type": "Source",
          "sourceStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "namespace": "csnr-devops-lab-deploy",
              "name": "webapp-builder:latest"
            },
            "env": [
              {
                "name": "MAVEN_ARGS",
                "value": "--offline"
              },
              {
                "name": "MAVEN_CLEAR_REPO",
                "value": "true"
              },
              {
                "name": "MAVEN_S2I_ARTIFACT_DIRS",
                "value": "target/"
              },
              {
                "name": "S2I_SOURCE_DEPLOYMENTS_FILTER",
                "value": "*.war"
              }
            ],
            "forcePull": true
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "showcase-java-web-app:latest"
          }
        },
        "resources": {
          "requests": {
            "cpu": "1",
            "memory": "1Gi"
          },
          "limits": {
            "cpu": "2",
            "memory": "2Gi"
          }
        },
        "postCommit": {},
        "nodeSelector": null
      },
      "status": {
        "lastVersion": 0
      }
    }
  ],
  "labels": {
    "template": "showcase-java-web-app"
  }
}
