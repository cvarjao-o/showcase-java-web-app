{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {},
  "objects": [
    {
      "kind": "BuildConfig",
      "apiVersion": "build.openshift.io/v1",
      "metadata": {
        "name": "showcase-tomcat",
        "creationTimestamp": null,
        "labels": {},
        "annotations": {}
      },
      "spec": {
        "triggers": [
          {
            "type": "ConfigChange"
          }
        ],
        "source": {
          "type": "Git",
          "git": {
            "uri": "https://github.com/cvarjao-o/showcase-java-web-app.git",
            "ref": "master"
          },
          "contextDir": "openshift/images/tomcat"
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "namespace": "csnr-devops-lab-deploy",
              "name": "tomcat:9.0.27-jdk8-openjdk"
            },
            "imageOptimizationPolicy": "SkipLayers"
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "showcase-tomcat:latest"
          }
        },
        "resources": {
          "requests": {
            "cpu": "1",
            "memory": "1Gi"
          },
          "limits": {
            "cpu": "2",
            "memory": "2Gi"
          }
        },
        "postCommit": {},
        "nodeSelector": null
      },
      "status": {
        "lastVersion": 0
      }
    }
  ],
  "labels": {
    "template": "showcase-java-web-app"
  }
}
